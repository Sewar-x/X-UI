{
  "name": "xw-ui",
  "version": "1.0.20",
  "description": "配置化组件库",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "exports": {
    "./element-plus": {
      "require": "./packages/element-plus/dist/xw-element-plus.cjs.js",
      "import": "./packages/element-plus/dist/xw-element-plus.es.js"
    },
    "./element-ui": {
      "require": "./packages/element-ui/dist/xw-element-ui.cjs.js",
      "import": "./packages/element-ui/dist/xw-element-ui.es.js"
    },
    "./ant-design": {
      "require": "./packages/ant-design/dist/xw-ant-design.cjs.js",
      "import": "./packages/ant-design/dist/xw-ant-design.es.js"
    },
    "./element-plus/*": {
      "require": "./packages/element-plus/dist/*",
      "import": "./packages/element-plus/dist/*"
    },
    "./element-ui/*": {
      "require": "./packages/element-ui/dist/*",
      "import": "./packages/element-ui/dist/*"
    },
    "./ant-design/*": {
      "require": "./packages/ant-design/*",
      "import": "./packages/ant-design/*"
    }
  },
  "scripts": {
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:build": "pnpm run -C docs docs:build && pnpm run -C packages/element-plus docs:build && pnpm run -C packages/element-ui docs:build &&  pnpm run -C packages/ant-design docs:build ",
    "docs:preview": "pnpm run -C docs docs:preview",
    "demo:dev-elp": "pnpm run -C packages/element-plus dev",
    "demo:dev-elu": "pnpm run -C packages/element-ui dev",
    "demo:dev-ant": "pnpm run -C packages/ant-design dev",
    "test:elp": "pnpm run -C test/packages/element-plus dev",
    "build": "pnpm run build:shared && pnpm run build:elp && pnpm run build:elu && pnpm run build:ant",
    "build:elp": "pnpm run -C packages/element-plus build",
    "build:elu": "pnpm run -C packages/element-ui build",
    "build:ant": "pnpm run -C packages/ant-design build",
    "build:shared": "pnpm run -C packages/shared build",
    "release": "node scripts/release.ts",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix ./"
  },
  "publishConfig": {
    "registry": "http://127.0.0.1:4873/"
  },
  "keywords": [
    "xw-ui",
    "vue",
    "react",
    "element",
    "element-plus",
    "ant-design"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {},
  "dependencies": {
    "@types/lodash": "^4.17.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^18.19.26",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "chalk": "^4.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.24.0",
    "execa": "^5.1.0",
    "fs-extra": "^10.1.0",
    "less": "^4.1.3",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-stylistic": "^0.4.5",
    "typescript": "^5.4.3",
    "unplugin-vue-macros": "^2.8.0",
    "vite": "^2.9.13",
    "vue-eslint-parser": "^9.4.2"
  }
}